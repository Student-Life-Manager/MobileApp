{
	"expo": {
		"name": "student-life-manager",
		"slug": "student-life-manager",
		"version": "2.2.5",
		"assetBundlePatterns": ["**/*"],
		"orientation": "portrait",
		"userInterfaceStyle": "light",
		"jsEngine": "hermes",
		"splash": {
			"backgroundColor": "#ddffe2",
			"image": "./src/assets/splash.png",
			"resizeMode": "contain"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"android": {
			"versionCode": 42
		},
		"androidNavigationBar": {
			"backgroundColor": "#ddffe2",
			"barStyle": "dark-content"
		},
		"androidStatusBar": {
			"backgroundColor": "#ddffe2",
			"barStyle": "dark-content"
		},
		"plugins": [
			"expo-dev-client",
			[
				"expo-barcode-scanner",
				{
					"cameraPermission": "Allow Student Life Manager to access camera."
				}
			]
		]
	}
}
